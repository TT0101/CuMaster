@model CuMaster.BusinessLibrary.ViewModels.ConversionTrackerListViewModel

<div class="container-fluid text-center">
    <div class="col-md-10 col-md-offset-1">
        <div class="row">
            <div class="alert alert-success hidden" id="trackerUpdateSuccess">
                Your tracker entry was updated successfully.
            </div>
            <div class="alert alert-danger hidden" id="trackerUpdateFailed">
                Your tracker entry update failed to process.  Please try again.
            </div>
        </div>
    </div>
    <div class="row">
        <div class="collapse-bar col-md-offset-1 col-md-10 bg-info text-left" style="min-height:25px;" id="trackerCollapse" data-toggle="collapse">
            <div class="text-left col-md-9 col-sm-7 col-xs-7">
                <label style="color:#eee;">Conversion Tracker</label>
            </div>
            <div class="text-right pull-right col-md-1" style="font-size:18px; color:#eee;">
                <span id="trackerCollapseToggle" class="glyphicon glyphicon-collapse-down"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="collapse-pane col-md-offset-1 col-md-10 col-sm-12 collapse show" id="trackerCollapsePane">
           
            <div class="pull-right col-md-8 text-right">
                @*<button class="btn btn-info" onclick="compareSelectedEntries()">Compare Selected <span class="badge" id="badgeCompare"></span></button>*@
               <button class='btn btn-primary' onclick='deleteAllEntries();'><span class='glyphicon glyphicon-trash'></span>&nbsp;Delete All</button>
            </div>
            <br/><br/>
            <table id="trackerTable" class="dataTable"></table>
        </div>
    </div>
</div>
